<web-fragment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="3.0"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd">
    <name>GuicedWebServices</name>
    <display-name>guiced-web-services</display-name>
    <description>Provides the implementation for Web Services into the Guice Domain Context GuicedInjection</description>
    <listener>
        <listener-class>
            com.sun.xml.ws.transport.http.servlet.WSServletContextListener
        </listener-class>
    </listener>
</web-fragment>
