                            <configuration>
                                <jvmVersion>9</jvmVersion>
                            </configuration>
                        </plugin>

                    </plugins>
                </pluginManagement>

                <resources>
                    <resource>
                        <directory>src/main/resources</directory>
                        <excludes>
                            <exclude>.gitignore</exclude>
                        </excludes>
                    </resource>
                    <resource>
                        <directory>src/jre8/resources</directory>
                        <excludes>
                            <exclude>.gitignore</exclude>
                        </excludes>
                    </resource>
